{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@types/*": [
        "./src/types/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
